	<script type='text/javascript'>
	// alert('test');

	// Smooth scrolling between the articles //

	var root = $('html, body');

	$('a.prev, #menu a, a.next').on('click', function(){
		var linkHref = $(this).attr('href');
		root.animate({
			scrollTop: $(linkHref).offset().top
		}, 1200, 'easeInOutExpo', function(){
			window.location.hash = linkHref;
		});
		return false;
	});


	//CLICKing on a menu nav button
	var menuButtons = $('#menu').find('a');
	menuButtons.on('click', function(){
		menuButtons.animate({'background-color' : '#d9edf6'}, 200, 'swing');
		$(this).animate({'background-color' : '#70B2C2'}, 100, 'swing');

	});
	//MENU Nav TOOLTIP on hover
	var tooltips = $('.tooltip');
	var menuNb;
	menuButtons.on('mouseenter', function(){
		 menuNb = menuButtons.index(this);
		$(tooltips[menuNb]).css({'opacity' : '0.2', 'display' : 'inline'}).animate({'right' : '9%', 'opacity' : '1'}, 200);
	});

	menuButtons.on('mouseleave', function(){
		$(tooltips[menuNb]).animate({'right' : '14%', 'opacity' : '0.2'}, 300, function(){
			$(this).css({'display' : 'none'});
		});
	});
	//Portal log-in box animation
	//'top': '-169px',
	var portal = $('#login-box');
	$(portal).on('mouseenter', function(){
		$(this).animate({'top' : '-175px'}, 720, 'easeOutElastic');
	});


	$(portal).on('mouseleave', function(){
		$(this).stop().animate({'top' : '-179px'}, 350, 'easeInOutExpo');
	});

	$(portal).on('click', function(){
		$(this).animate({'top':'0px'}, 200, 'easeInOutExpo');
	});



	// Animating the backgrounds //
	$(window).on('scroll', function(){
		moveBackground();
	});


	function moveBackground() {
		var scrolled = $(window).scrollTop();
		$('#clouds-high').css({'top' : - 0.3 * scrolled + 'px',
						  'right' : 0.04 * scrolled + 'px'
						});
		$('#clouds-low').css({'top' : - 0.2 * scrolled + 'px',
						  'right' : 0.06 * scrolled + 'px'
						});
		$('#balloon1').css({
							'top' : 63 + 0.114 * scrolled + '%',
							'left' : -3 + 0.002 * scrolled + '%'
		});

		$('#sun').css({'top' : 1 + 0.05 * scrolled + '%',
			'-webkit-transform' : 'rotate(' + (scrolled/$(window).height())*80 + 'deg)'
		});

		// $('#logo').css ({
		// 	'top' : 0.19 * scrolled + 'px',
		// 	'left' : -0.1 * scrolled + 'px'

		// });

		$('#airplane').css({
			'left' : ((32 + 0.04 * scrolled)< 88 ? (32 + 0.04 * scrolled) : 88 ) + '%',
			'top' : 12 + 0.02 * scrolled + '%'
		});

		$('#logo').css({
			'top':  + 0.2 * scrolled + 'px'
		});
	}


	// Photo Gallery

	var displayedPhotos = $('.displayed img');
	var slideshowPhotos = $('#slideshow img');
	var index = 0;
	var positionX = 0;
	var positionY = 0;
	var currentImg = $(slideshowPhotos[index]);

	displayedPhotos.on('click', function (){
		index = $(displayedPhotos).index(this);
		currentImg = $(slideshowPhotos[index]);
		// positionX = $(this).offset().left;
		// positionY = $(this).offset().top;

		//bringing out the background for the slideshow
		$('body').css({"overflow" : "hidden"}).animate({scrollTop: $("#photos").offset().top }, 300, "swing", 
			function(){ $('#slideshow').css({'display' : 'inherit', 
								'opacity' : 0.1, 
								'bottom' : '48%', 
								'right' : '48%', 
								'top': '48%', 
								'left': '48%'})
							.animate({'opacity' : 1, 
									'bottom' : '0%', 
									'right' : '21%', 
									'top': '14%', 
									'left': '21%'}, 900, 'easeOutQuad', function(e){

										//places the first photo from the slideshow on the screen
										currentImg.css({'height' : '80%'});
										var CurrentImgWidth = currentImg.width();
										var CurrentImgHeight = currentImg.height();

										currentImg.css({'opacity' : 0.4, 
													'height' : '22%', 
													'display': 'inherit',
													'left': (($('#slideshow').width())/ 2 ) + 'px',
													'top' : (($('#slideshow').height())/ 2 ) + 'px'
												})
												.animate({
													'height' : '80%',
													'opacity' : 1, 
													'left': (($('#slideshow').width() - CurrentImgWidth)/ 2 ) + 'px',
													'top' : (($('#slideshow').height() - CurrentImgHeight)/ 2 ) + 'px'
													}, 500, 'easeOutCubic');
										});

						});
	});

	//slideshow - go through the photos with clicks
	slideshowPhotos.on('click', function(e){
		var offset = $(this).offset();
		var mouseClickX = e.clientX - offset.left;
		if((mouseClickX > $(this).width() / 2)) {					
			goToNextImage();
		} else {
			goToPreviousImage();
		}
	});

	function goToNextImage() {
		if(index < slideshowPhotos.length - 1) {
			currentImg.animate({'opacity' : 0}, 200).css({'display' : 'none'});
			// index ++;
			currentImg = $(slideshowPhotos[++index]);
			currentImg.css({'display' : 'inherit', 'opacity' : 0}).animate({'opacity' : 1}, 200);

		} else {
			currentImg.animate({'opacity' : 0}, 200).css({'display' : 'none'});
			index = 0;
			currentImg = $(slideshowPhotos[index]);
			currentImg.css({'display' : 'inherit', 'opacity' : 0}).animate({'opacity' : 1}, 200);
		}
	}


	function goToPreviousImage() {
		if(index > 0) {
			currentImg.animate({'opacity' : 0}, 200).css({'display' : 'none'});
			// index ++;
			currentImg = $(slideshowPhotos[--index]);
			currentImg.css({'display' : 'inherit', 'opacity' : 0}).animate({'opacity' : 1}, 200);

		} else {
			currentImg.animate({'opacity' : 0}, 200).css({'display' : 'none'});
			index = slideshowPhotos.length-1;
			currentImg = $(slideshowPhotos[index]);
			currentImg.css({'display' : 'inherit', 'opacity' : 0}).animate({'opacity' : 1}, 200);
		}
	}

	//exit the slideshow with esc key or a click outside the slideshow
	$('body').on('click', function(e){
				if(e.target.nodeName.toLowerCase() != 'img') {
					exitSlideshow();
				}
			});

	$('body').on('keydown', function(e){
		if(e.which == 27) {
			exitSlideshow();
		}
	});

	function exitSlideshow() {
		$('#slideshow').css({'display' : 'none'});
					$(slideshowPhotos).css({'display' : 'none'});
					$("body").css({"overflow" : "visible"});
	}
	</script>
